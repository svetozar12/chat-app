{
  "name": "@chat-app/sdk",
  "version": "1.0.0",
  "description": "gql sdk and rest api sdk",
  "main": "dist/sdk.umd.js",
  "module": "dist/sdk.es.js",
  "types": "dist/sdk.es.d.ts",
  "license": "MIT",
  "scripts": {
    "rest-codegen": "openapi-generator-cli generate -i ./rest.yaml -o generated/rest/openapi -g typescript-axios --additional-properties=supportsES6=true,npmVersion=6.9.0,typescriptThreePlus=true",
    "codegen": "yarn rest-codegen",
    "prebuild": "yarn codegen",
    "build": "rimraf dist && tsc && vite build"
  },
  "dependencies": {
    "@chat-app/graphql-server": "*",
    "@openapitools/openapi-generator-cli": "^2.5.1"
  },
  "devDependencies": {
    "vite": "^2.9.5",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-dts": "^1.1.0",
    "typescript": "^4.8.2"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
